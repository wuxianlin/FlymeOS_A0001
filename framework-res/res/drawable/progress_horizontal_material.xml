<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <nine-patch android:src="@drawable/progress_mtrl_alpha" android:tint="?colorControlNormal" android:alpha="?disabledAlpha" />
    </item>
    <item android:id="@id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/progress_mtrl_alpha" android:tint="?colorControlActivated" android:alpha="?disabledAlpha" />
        </scale>
    </item>
    <item android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/progress_mtrl_alpha" android:tint="?colorControlActivated" />
        </scale>
    </item>
</layer-list>